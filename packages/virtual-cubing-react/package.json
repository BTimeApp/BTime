{
  "name": "@btime/virtual-cubing-react",
  "version": "0.1.0",
  "description": "Virtual Cube for React apps",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --tsconfig tsconfig.json",
    "dev": "tsup src/index.ts --watch --tsconfig tsconfig.json"
  },
  "keywords": [
    "rubiks",
    "cube"
  ],
  "license": "MIT-only",
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/three": "^0.182.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "cubing": "^0.61.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "three": "^0.182.0"
  }
}
