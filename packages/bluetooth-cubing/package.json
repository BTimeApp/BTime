{
  "name": "@btime/bluetooth-cubing",
  "version": "0.1.0",
  "description": "Library for bluetooth cubes and timers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --tsconfig tsconfig.json",
    "dev": "tsup src/index.ts --watch --tsconfig tsconfig.json"
  },
  "keywords": [
    "bluetooth",
    "rubiks",
    "cube"
  ],
  "license": "GPL-3.0-only",
  "devDependencies": {
    "cac": "^6.7.14",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "cubing": "^0.58.0"
  }
}
